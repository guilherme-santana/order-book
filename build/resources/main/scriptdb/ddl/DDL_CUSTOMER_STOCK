-- order_book.customer_stock definition

CREATE TABLE `customer_stock` (
  `id` bigint NOT NULL AUTO_INCREMENT,
  `amount` int DEFAULT NULL,
  `asset_id` bigint DEFAULT NULL,
  `customer_id` bigint DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `FKruedptf36ik5151caonacpxve` (`asset_id`),
  KEY `FKax6vriw6o7b2pam02dn30fwni` (`customer_id`),
  CONSTRAINT `FKax6vriw6o7b2pam02dn30fwni` FOREIGN KEY (`customer_id`) REFERENCES `customer` (`id`),
  CONSTRAINT `FKruedptf36ik5151caonacpxve` FOREIGN KEY (`asset_id`) REFERENCES `asset` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;